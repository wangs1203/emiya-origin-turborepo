{
  "name": "@emiya-turbo-origin/rc",
  "version": "0.0.1-alpha.0",
  "description": "ðŸš€ Beautiful and modern React UI library.",
  "homepage": "http://",
  "author": "Emiya Leon <wangs.coder@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "components",
    "react components",
    "react ui",
    "ui",
    "design-system",
    "antd"
  ],
  "publishConfig": {
    "types": "./dist/types/index.d.ts",
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/wangs1203/emiya-origin-turborepo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+http://github.com/wangs1203/emiya-origin-turborepo.git",
    "directory": "packages/rc"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/lib/index.js",
  "module": "./dist/es/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js"
    },
    "./base-loader": {
      "types": "./dist/types/base-loader/index.d.ts",
      "import": "./dist/es/base-loader/index.js",
      "require": "./dist/lib/base-loader/index.js"
    },
    "./button": {
      "types": "./dist/types/button.d.ts",
      "import": "./dist/es/button.js",
      "require": "./dist/lib/button.js"
    },
    "./code": {
      "types": "./dist/types/code.d.ts",
      "import": "./dist/es/code.js",
      "require": "./dist/lib/code.js"
    },
    "./card": {
      "types": "./dist/types/card.d.ts",
      "import": "./dist/es/card.js",
      "require": "./dist/lib/card.js"
    },
    "./container": {
      "types": "./dist/types/container/index.d.ts",
      "import": "./dist/es/container/index.js",
      "require": "./dist/lib/container/index.js"
    },
    "./global-loader": {
      "types": "./dist/types/global-loader/index.d.ts",
      "import": "./dist/es/global-loader/index.js",
      "require": "./dist/lib/global-loader/index.js"
    }
  },
  "scripts": {
    "build": "modern build",
    "dev": "modern build -w",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf .turbo node_modules dist",
    "upgrade": "modern upgrade",
    "new": "modern new"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "ahooks": "^3.8.0",
    "antd": "^5.9.0",
    "framer-motion": "^11.0.22",
    "tailwindcss": "^3.4.3"
  },
  "peerDependencies": {
    "@ant-design/icons": ">=5.3.7",
    "ahooks": ">=3.8.0",
    "antd": ">=5.9.0",
    "clsx": ">=2.1.1",
    "framer-motion": ">=10.0.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "tailwindcss": ">=3.4.3"
  },
  "devDependencies": {
    "@emiya-turbo-origin/eslint-config": "workspace:*",
    "@emiya-turbo-origin/tailwind-config": "workspace:*",
    "@emiya-turbo-origin/typescript-config": "workspace:*",
    "@emiya-turbo-origin/utils": "workspace:*",
    "@modern-js/core": "^2.49.3",
    "@modern-js/module-tools": "^2.49.3",
    "@modern-js/plugin-tailwindcss": "^2.49.3",
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.1",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6"
  }
